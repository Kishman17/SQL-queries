Запросы для выгрузки данных с БД и создания отчета Qlik Sense
1. select doc.r_object_id,
	doc.r_modify_date,
	doc.dss_branch, 
	doc.dss_object_type, 
    org.dss_name, 
    doc.dss_reg_number as reg_name,
    doc.dsdt_reg_date as reg_date,
    doc.dss_description as description,
    doc.dss_description_en as description_en,
    doc.dsdt_exec_date, 
    doc.dss_subject_code as subject,
    psp.dss_name as psp_dss_name,
    psp.parent_id,
    psp.child_id,
    ps.dsdt_start_date,
	ps.object_name,
    psp.dss_perf_status,
    doc.dss_status,
    doc.dss_document_type,
    doc.dss_comment,
    doc.dss_esign,
    re.dss_user_name as Registrar
  from io_total_folder doc  
  left join bd_dms_folder_correspo bdfc on doc.r_object_id = bdfc.parent_id
  left join bd_organization org on bdfc.child_id = org.r_object_id
  left join bd_dms_folder_ph_regis re on doc.r_object_id= re.parent_id
  left join bd_proc_step ps on ps.dss_case_id = doc.r_object_id
  left join bd_proc_step_pos psp on psp.parent_id = ps.r_object_id
  left join bd_position_history rp on rp.r_object_id = psp.child_id
    Where doc.r_object_id = '*******' and doc.dss_object_type = 'incoming' and psp.dss_name != ' '
/////////////////////////////////
2. select a.*, co.dss_user_name, it.dss_user_name, si.dss_user_name, re.dss_user_name
from (select io.r_object_id as io_object_id,
r_modify_date as io_modify_date,
io.dss_branch as io_branch,
di.dss_value as di_value,
io.dss_object_type as io_object_type,
org.dss_name as org_dss_name ,
io.dss_reg_number as io_reg_number,
io.dsdt_reg_date as io_reg_date,
io.dss_description as io_description,
io.dsdt_exec_date as io_exec_date,
io.dss_subject_code as io_subject_code,
io.dss_status as io_status,
io.dss_document_type as io_document_type,
io.dss_comment as io_comment,
io.dss_esign as io_esign
from io_total_folder io, bd_simple_directory di, bd_organization org
where io.dss_branch = di.dss_branch
and io.dss_corr_code = org.dss_code
and di.dss_directory_type = 'branch'
and io.dss_reg_number !=''
and io.dss_object_type = 'outgoing') a, bd_dms_folder_ph_coord co, bd_dms_folder_ph_regis re, bd_dms_folder_ph_signe si, bd_dms_folder_ph_initi it
where a.io_object_id = co.parent_id
and a.io_object_id = re.parent_id
and a.io_object_id = si.parent_id
and a.io_object_id = it.parent_id
